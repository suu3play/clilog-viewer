{
  "name": "@suu3play/clilog-viewer",
  "version": "2.0.0",
  "description": "AI CLI conversation log converter and viewer with SQLite cache and real-time monitoring",
  "main": "log_converter.py",
  "scripts": {
    "start": "cd viewer && python app.py",
    "convert": "python log_converter.py",
    "convert:force": "python log_converter.py --force",
    "list": "python log_converter.py --list",
    "build-cache": "cd viewer && python message_cache.py --build-cache"
  },
  "keywords": [
    "ai",
    "cli",
    "log",
    "converter",
    "viewer",
    "markdown",
    "chat",
    "claude",
    "github-copilot",
    "chatgpt",
    "sqlite",
    "flask",
    "realtime"
  ],
  "author": "suu3play",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suu3play/clilog-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/suu3play/clilog-viewer/issues"
  },
  "homepage": "https://github.com/suu3play/clilog-viewer#readme",
  "engines": {
    "python": ">=3.6"
  },
  "files": [
    "log_converter.py",
    "log_converter_config.ini",
    "install_task_scheduler.bat",
    "install_task_scheduler.ps1",
    "uninstall_task_scheduler.bat",
    "uninstall_task_scheduler.ps1",
    "viewer/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
